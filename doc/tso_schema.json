      "$id": "#/properties/bus",
      "type": "array",
      "title": "The Bus Schema",
      "items": {
        "$id": "#/properties/bus/items",
        "type": "array",
        "title": "The Items Schema",
        "items": {
          "$id": "#/properties/bus/items/items",
          "type": "integer",
          "title": "The Items Schema",
          "default": 0,
          "examples": [
            1,
            3,
            15167.5,
            3079.89223516626,
            0,
            5000,
            1,
            1,
            0,
            345,
            1,
            1.1,
            0.9
          ]
