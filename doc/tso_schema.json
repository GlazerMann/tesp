{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/root.json",
  "type": "object",
  "title": "The Root Schema",
  "required": [
    "version",
    "baseMVA",
    "StartTime",
    "Tmax",
    "Period",
    "dt",
    "pf_dc",
    "opf_dc",
    "bus",
    "gen",
    "branch",
    "areas",
    "gencost",
    "FNCS",
    "UnitsOut",
    "BranchesOut",
    "swing_bus"
  ],
  "properties": {
    "version": {
      "$id": "#/properties/version",
      "type": "integer",
      "title": "The Version Schema",
      "default": 0,
      "examples": [
        2
      ]
    },
    "baseMVA": {
      "$id": "#/properties/baseMVA",
      "type": "integer",
      "title": "The Basemva Schema",
      "default": 0,
      "examples": [
        100
      ]
    },
    "StartTime": {
      "$id": "#/properties/StartTime",
      "type": "string",
      "title": "The Starttime Schema",
      "default": "",
      "examples": [
        "2013-07-01 00:00:00"
      ],
      "pattern": "^(.*)$"
    },
    "Tmax": {
      "$id": "#/properties/Tmax",
      "type": "integer",
      "title": "The Tmax Schema",
      "default": 0,
      "examples": [
        86400
      ]
    },
    "Period": {
      "$id": "#/properties/Period",
      "type": "integer",
      "title": "The Period Schema",
      "default": 0,
      "examples": [
        300
      ]
    },
    "dt": {
      "$id": "#/properties/dt",
      "type": "integer",
      "title": "The Dt Schema",
      "default": 0,
      "examples": [
        60
      ]
    },
    "pf_dc": {
      "$id": "#/properties/pf_dc",
      "type": "integer",
      "title": "The Pf_dc Schema",
      "default": 0,
      "examples": [
        1
      ]
    },
    "opf_dc": {
      "$id": "#/properties/opf_dc",
      "type": "integer",
      "title": "The Opf_dc Schema",
      "default": 0,
      "examples": [
        1
      ]
    },
