    "areas": {
      "$id": "#/properties/areas",
      "type": "array",
      "title": "The Areas Schema"
    },
    "gencost": {
      "$id": "#/properties/gencost",
      "type": "array",
      "title": "The Gencost Schema",
      "items": {
        "$id": "#/properties/gencost/items",
        "type": "array",
        "title": "The Items Schema",
        "items": {
          "$id": "#/properties/gencost/items/items",
          "type": "integer",
          "title": "The Items Schema",
          "default": 0,
          "examples": [
            2,
            0,
            0,
            3,
            0.005,
            40,
            0
          ]
        }
      }
    },
    "FNCS": {
      "$id": "#/properties/FNCS",
      "type": "array",
      "title": "The Fncs Schema",
      "items": {
        "$id": "#/properties/FNCS/items",
        "type": "array",
        "title": "The Items Schema",
        "items": {
          "$id": "#/properties/FNCS/items/items",
          "type": "integer",
