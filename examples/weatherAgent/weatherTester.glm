clock{
  timezone EST+5EDT;
  timestamp '2013-07-01 14:00:00';
  stoptime '2013-07-01 15:00:00';
}

module connection;
module climate;
module generators;
module residential;

object climate {
  name exampleWeather;
  temperature 0.0;
  humidity 0.0;
  solar_dir 0.0;
  solar_diff 0.0;
  wind_speed 0.0;
  pressure 0.0;
  interpolate QUADRATIC;
};
object solar {
	weather exampleWeather;
    generator_mode SUPPLY_DRIVEN;
    generator_status ONLINE;
    panel_type SINGLE_CRYSTAL_SILICON;
    efficiency 0.2;
    area 2500 sf;
}

object house {
	name weatherHouse;
}

#ifdef USE_FNCS
object fncs_msg {
  name gridlabdweatheragent;
  configure weather_FNCS_Config.txt;
  option "transport:hostname localhost, port 5570";
  aggregate_subscriptions true;
  aggregate_publications true;
}
#endif